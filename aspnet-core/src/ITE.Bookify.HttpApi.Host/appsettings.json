    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AuthServer": {
    "Authority": "https://localhost:44397",
    "RequireHttpsMetadata": false,
    "SwaggerClientId": "Bookify_Swagger"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "VOgIKLhSjzYmjNEw"
  },
  "DataSeeding": {
